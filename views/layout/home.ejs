<div id="searchcontain">
  <img class="projector" src="../../styles/images/projector.jpg">
  <form class="search-form" action="/show" method="post">
    <label id="labelSearch">Movie Search: 
      <input type="text" name="movieSearch" id="movieSearch"/> 
    </label>
    <button id="searchbut">Submit</button>
  </form>
</div>
<h2 id="poptitle">Popular Movies</h2>
<form>
    <select class="sort">
      <option value="default">Sort By</option>
      <option value="default">Rating</option>
      <option value="default">Popularity</option>
      <option value="default">Ranking</option>
    </select>
  </form>
<ul id="popularMovies">
    <% popularMovies.forEach(movie => { %>
      <div id="imgcontain">
        <li  id="listimage" data-id="<%= popularMovies.id %>">
        <img class="homemovies" src="<%= movie.image_url %>">
        <!-- <h3 id="homemtitle"><%= movie.title %></h3> -->
        </li>
      </div>
    <% }) %>
  </ul>