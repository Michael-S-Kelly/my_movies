<main>
  <h2> Search Results</h2>
  <section class ="all-api-movies" id ="test">
      <% renderedMovies.forEach( data => { %>
        <section class ="api-movies">
          <h3 class="title"><%= data.title %></h3>
          <p><%= data.released_on %></p>
          <img src=<%= data.image_url %>>
